{
    "name": "husky-eats",
    "private": true,
    "packageManager": "pnpm@10.15.0",
    "scripts": {
        "start:backend": "concurrently \"pnpm dev:backend\" \"pnpm dlx localtunnel --port 4000 --subdomain huskyeats\"",
        "start:dev": "concurrently --raw \"pnpm dev:frontend\" \"pnpm dev:backend\" \"pnpm dlx localtunnel --port 4000 --subdomain huskyeats\"",
        "dev:backend": "pnpm dlx tsx apps/backend/src/index.ts",
        "dev:frontend": "pnpm --filter ./apps/frontend start"
    },
    "workspaces": [
        "apps/*"
    ],
    "devDependencies": {
        "concurrently": "^9.2.1",
        "localtunnel": "^2.0.2"
    },
    "dependencies": {
        "react-native-safe-area-context": "^5.4.0",
        "react-native-screens": "~4.11.1"
    }
}
